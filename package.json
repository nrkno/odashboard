{
  "name": "Odashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "node scripts/preinstall.js",
    "test:lint": "eslint . --ext .js --ignore-path .gitignore --cache",
    "test": "npm run test:lint --silent && mocha --recursive test/",
    "test-travis": "mocha --recursive test/",
    "build": "webpack --progress",
    "watch": "webpack --watch",
    "watch-tests": "mocha --recursive test/ --watch",
    "start": "nodemon app.js"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-route": "^1.5.8",
    "chart.js": "^2.3.0",
    "ejs": "^2.0.0",
    "express": "^4.14.0",
    "googleapis": "^17.1.0",
    "httpntlm": "^1.6.1",
    "lodash": "^4.16.4",
    "moment": "^2.15.1",
    "request": "~2.53.0",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.5.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "eslint": "^3.7.1",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.6",
    "webpack": "^1.12.9"
  }
}
